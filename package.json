{
  "name": "ryosuke-gatsby-blog",
  "description": "Personal Portfolio and Blog of Ryosuke",
  "version": "1.0.0",
  "author": "Ryosuke <ryosuke.san.hana@gmail.com>",
  "keywords": [
    "react",
    "styled-components",
    "gatsby",
    "blog",
    "portfolio",
    "rebass"
  ],
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "clean": "rm -rf .cache && npm run develop",
    "develop": "nodemon --exec \"gatsby develop\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint . --ext ts --ext tsx",
    "storybook": "start-storybook",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "functions/*",
      "schema.json",
      "src/*",
      "content/*",
      "static/*"
    ],
    "delay": "1500"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.4",
    "@mdx-js/react": "^1.6.4",
    "@mdx-js/tag": "^0.20.3",
    "@rebass/forms": "^4.0.6",
    "@sendgrid/mail": "^7.1.1",
    "array-flat-polyfill": "^1.0.1",
    "babel-plugin-styled-components": "^1.10.7",
    "dotenv": "^8.2.0",
    "gatsby": "^2.22.11",
    "gatsby-image": "^2.4.5",
    "gatsby-link": "^2.4.3",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-feed": "^2.5.3",
    "gatsby-plugin-google-analytics": "^2.3.2",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-mdx": "^2.14.1",
    "gatsby-plugin-offline": "^3.2.7",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-plugin-sitemap": "^2.4.3",
    "gatsby-plugin-styled-components": "^3.3.2",
    "gatsby-plugin-twitter": "^2.3.2",
    "gatsby-remark-copy-linked-files": "^2.3.3",
    "gatsby-remark-images": "^3.3.8",
    "gatsby-remark-prismjs": "^3.5.2",
    "gatsby-remark-smartypants": "^2.3.2",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-json": "^2.4.3",
    "gatsby-transformer-remark": "^2.8.13",
    "gatsby-transformer-sharp": "^2.5.3",
    "gh-pages": "^2.2.0",
    "prism-react-renderer": "^1.1.1",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-data-table-component": "^6.9.2",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-live": "^2.2.2",
    "react-mailchimp-subscribe": "^2.1.0",
    "rebass": "^4.0.7",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-backgrounds": "^5.3.19",
    "@storybook/addon-events": "^5.3.19",
    "@storybook/addon-jest": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addon-options": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/rebass": "^4.0.6",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^5.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-react": "^7.20.0",
    "gatsby-plugin-typescript": "^2.4.3",
    "graphql-typescript-definitions": "^0.20.2",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3",
    "typescript-styled-plugin": "^0.15.0"
  }
}
